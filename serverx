server {
       listen 80;
       listen [::]:80;

       server_name serverx.nrtsx.tk;

       root /var/www/server;
       index index.html;

       location / {
               try_files $uri $uri/ =404;
       }
       location ~ \.php$ {
               include snippets/fastcgi-php.conf;
               fastcgi_pass unix:/run/php/php8.0-fpm.sock
               fastcgi_pass 127.0.0.1:9000;
       }
}
